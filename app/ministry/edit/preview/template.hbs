                        <div class="panel-body">
                            <h4> </h4>
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Name</th> 
                                        <th>Age</th>
                                        <th>Gender</th>
										<th>Religious Affiliation</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#if believers}}
                                        {{#each believers}}
                                        <tr>
                                            <td>{{believerName}}</td> 
                                            <td>{{age}}</td>
                                            <td>{{gender}}</td> 
											<td>{{religiousAffiliation}}{{#if religiousAffiliationWasOther}}-{{religiousAffiliationWasOther}}{{/if}}</td>
                                        </tr>
                                        {{/each}}
                                    {{else}}
										{{#if noNewBelieversThisMonth}}
