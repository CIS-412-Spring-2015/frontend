                            {{else}}
                                <td class="pleaseFillInPreview">(Please Fill In)</td>
                            {{/if}}
                            
                            {{#if peopleDoor}}
                                <td>{{peopleDoor}}</td>
                            {{else}}
                                <td class="pleaseFillInPreview">(Please Fill In)</td>
                            {{/if}}
                            
                            {{#if peopleMore}}
                                <td>{{peopleMore}}</td>
                            {{else}}
                                <td class="pleaseFillInPreview">(Please Fill In)</td>
                            {{/if}}
                            <td>{{peopleTotal}}</td>
                        </tr>
                        <tr>
                            <th>Bibles Given Out</th>
                            {{#if bibleBedside}}
                                <td>{{bibleBedside}}</td> 
                            {{else}}
                                <td class="pleaseFillInPreview">(Please Fill In)</td>
                            {{/if}}
                            
                            {{#if biblePlayroom}}
                                <td>{{biblePlayroom}}</td>
                            {{else}}
                                <td class="pleaseFillInPreview">(Please Fill In)</td>
                            {{/if}}
                            
                            {{#if bibleJesus}}
                                <td>{{bibleJesus}}</td> 
                            {{else}}
