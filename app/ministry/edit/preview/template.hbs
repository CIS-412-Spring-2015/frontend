<div role="tabpanel" class="tab-pane" id="preview">
      <div class="panel">
          <div class="panel-heading">
              <h3 class="panel-title">
                Preview
              </h3>
          </div>
          <div class="panel-body detail-section-content">

<!-- Hospital -->              
              <div class="panel">
                  <div class="panel-heading" role="tab" id="hospitalPreview">
                      <a data-toggle="collapse" href="#hospitalPreviewCollapse" aria-expanded="true" aria-controls="hospitalPreviewCollapse"> 
                          <h3 class="panel-title headingsForPreview">
                              Hospital 
                              <!--{#if}}-->
                                <span class="octicon octicon-triangle-down pull-right"></span>
                              <!--{else}}
                                <span class="octicon octicon-triangle-up pull-right"></span>
                              {/if}}-->
