{{#em-form model=this submit_button=false }}
	<label>Name<span class="redWordSubmission">*</span></label>
    {{em-input property="participantName" placeholder="Enter the name of the event participant" class="required"}}
	
	<label>Church/Organization<span class="redWordSubmission">*</span></label>
    {{em-input property="organization" placeholder="Enter name of participant's church or organization" class="required"}}

	<div class="form-group">
		 <label>Gender<span class="redWordSubmission">*</span></label>
         {{ext-radio name=gender content=genderList value=gender showInline=true class="required"}}
    </div>
{{/em-form}}
