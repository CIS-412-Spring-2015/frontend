{{#em-form model=this submit_button=false }}
    {{em-input label="Name" property="eventName" placeholder="Enter the name of the event" class="required"}}
    {{date-picker label="Date" property="date" minDate=startOfReportMonth maxDate=endOfReportMonth placeholder="Click to enter the date of the event" class="required"}}
    {{em-input label="Location" property="location" placeholder="Enter the location of the event" class="required"}}
	{{em-text label="Description" property="description" placeholder="Enter a description of the leadership event" class="required" rows=3 }}
{{/em-form}}
